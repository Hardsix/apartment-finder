{
  "name": "apartment-finder",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS=--max-old-space-size=10000 node --max-old-space-size=10000 ./dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/.bin/typeorm",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:run:cloud": "npm run typeorm migration:run -- --config ormconfig.cloud.js",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.15",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/testing": "7.6.15",
    "@nestjs/typeorm": "7.1.5",
    "@types/cron": "^1.7.3",
    "@types/events": "3.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.108",
    "@types/node": "13.9.1",
    "@types/sinon": "9.0.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "apify": "2.0.7",
    "bluebird": "3.7.2",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "26.4.2",
    "joi": "17.3.0",
    "jsdom": "16.6.0",
    "lodash": "4.17.15",
    "p-queue": "6.6.2",
    "pg": "8.4.2",
    "prettier": "2.0.5",
    "puppeteer": "10.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "slack-notify": "^0.1.7",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.4",
    "ts-jest": "26.5.5",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "type-fest": "^2.10.0",
    "typeorm": "0.2.29",
    "typescript": "4.2.4"
  },
  "devDependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
